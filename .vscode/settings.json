{
  "files.exclude": {
    "*.egg-info": true,
    "**/__pycache__/": true,
    "**/.cache/": true,
    "**/.coverage": true,
    "**/.pytest_cache/": true,
    "**/.venv/": true,
    "**/dist/": true,
    "**/node_modules/": true,
    "setup.py": true
  },
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.enabled": true,
  "editor.formatOnSave": true,
  "python.linting.lintOnSave": true,
  "cSpell.words": [
    "CIRCLECI",
    "aiofiles",
    "aiohttp",
    "jsonify",
    "sanic"
  ]
}
